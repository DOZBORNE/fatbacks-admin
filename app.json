{
  "expo": {
    "name": "Fatbacks Admin",
    "slug": "fatbacksAdmin",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/plato-app-icon.png",
    "scheme": "admin",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/plato-splash-app-screen.png",
      "resizeMode": "contain",
      "backgroundColor": "#222222"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}
